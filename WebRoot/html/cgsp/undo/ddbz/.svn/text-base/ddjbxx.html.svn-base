<!doctype html>
<html lang="en" ng-app="myApp">
	<head>
		<meta charset="GBK">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"  id="viewport">
		<meta name="format-detection" content="telephone=no">
		<!-- 避免缓存cache -->
		<meta http-equiv="cache-control" content="max-age=0" />
		<meta http-equiv="cache-control" content="no-cache" />
		<meta http-equiv="expires" content="0" />
		<meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
		<meta http-equiv="pragma" content="no-cache" />
		<title>EPS在线</title>
		<link rel="stylesheet" href="../../../../resource/css/common.css" type="text/css" />
		<link rel="stylesheet" type="text/css" href="../../../../resource/css/style.css"/>
		<link rel="stylesheet" type="text/css" href="../../../../resource/css/layer.css" id="layui_layer_skinlayercss"/>
	</head>
	<body class="secPage">
		<header class="main-header">
			<span class="pre-header"></span>
			<h1>订单基本信息</h1>
		</header>
		<div class="second-wrap" ng-controller="ddjbxx">
			<table class="table table-s2">
				<tr>
					<td>主合同类型</td>
					<td ng-bind="data.zhtlx"></td>
				</tr>
				<tr>
					<td>订单类别</td>
					<td ng-bind="data.ddlb"></td>
				</tr>
				<tr>
					<td>订单付款类别</td>
					<td ng-bind="data.ddfklb"></td>
				</tr>
				<tr>
					<td>合同编号</td>
					<td ng-bind="data.htbh"></td>
				</tr>
				<tr>
					<td>合同名称</td>
					<td ng-bind="data.htmc"></td>
				</tr>
				<tr>
					<td>供应商</td>
					<td ng-bind="data.gys"></td>
				</tr>
				<tr>
					<td>乙方经办人</td>
					<td ng-bind="data.yfjbr"></td>
				</tr>
				<tr>
					<td>订单编号</td>
					<td ng-bind="data.ddbh"></td>
				</tr>
				<tr>
					<td>订单名称</td>
					<td ng-bind="data.ddmc"></td>
				</tr>
				<tr>
					<td>订单创建依据</td>
					<td><a href="" ng-bind="data.ddcjyj.name"></a></td>
				</tr>
			</table>
		</div>	
		<!--色环加载-->
	    <div class="loader">
		    <div class="loaderLayer">
		        <div class="loader-inner line-spin-fade-loader">
		          <div></div>
		          <div></div>
		          <div></div>
		          <div></div>
		          <div></div>
		          <div></div>
		          <div></div>
		          <div></div>
		        </div>
		    </div>
	    </div>
		<script src="../../../../resource/js/system/jquery-2.2.0.min.js"></script>
		<script src="../../../../resource/js/angular.min.js"></script>
		<script src="../../../../resource/js/system/fastclick.js"></script>
		<script src="../../../../resource/js/system/layer.js"></script>
		<script src="../../../../resource/js/system/common.js"></script>
		<script src="../../../../resource/js/publicDo.js"></script>
		<script>
			var app=angular.module("myApp",[]);
			app.controller('ddjbxx',function($scope,$http){
				$http.get("ddbzjbxx.json").success(function(response){
					$('.loader').hide();
					iLoadJson.iLoad0=1;//加载成功
					$scope.data=response;
					
				}).error(function(){
					$('.loader').hide();
					layer.alert("加载出错啦")
				});
			});
		</script>
	</body>
</html>
